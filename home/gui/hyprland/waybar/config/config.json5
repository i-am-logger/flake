// json5
[
	{
		// TOP BAR
		"layer": "top",
		"position": "top",
		"height": 32,
		"modules-left": [
			"hyprland/workspaces",
			"custom/right-arrow-dark",
		],
		"modules-center": [
			"custom/left-arrow-dark",
			"clock",
			"custom/weather",
			"custom/right-arrow-dark"
		],
		"modules-right": [
			"custom/left-arrow-dark",
			// "privacy",
			// "custom/left-arrow-light",
			// "custom/left-arrow-dark",
			"network",
			"custom/left-arrow-light",
			"custom/left-arrow-dark",
			"pulseaudio",
			"custom/left-arrow-light",
			"custom/left-arrow-dark",
			"cava",
			"custom/left-arrow-light",
			"custom/left-arrow-dark",
			"battery",
			// "upower",
			"custom/left-arrow-light",
			"custom/left-arrow-dark",
			"tray"
		],
		"custom/left-arrow-dark": {
			"format": "",
			"tooltip": false
		},
		"custom/left-arrow-light": {
			"format": "",
			"tooltip": false
		},
		"custom/right-arrow-dark": {
			"format": "",
			"tooltip": false
		},
		"custom/right-arrow-light": {
			"format": "",
			"tooltip": false
		},
		"hyprland/workspaces": {
			"active-only": false,
			"format-window-separator": "|",
			"format": "{name}"
		},
		"clock": {
			"internal": 1,
			"format": "{:%A | %r | %d-%m-%Y}",
			"tooltip": false
		},
		"custom/weather": {
			"format": "| {}",
			"tooltip": true,
			"interval": 300,
			"exec": "$HOME/.config/waybar/scripts/waybar-weather.py",
			"return-type": "json"
		},
		"privacy": {
			"icon-spacing": 4,
			"icon-size": 24,
			"transition-duration": 250,
			"modules": [
				{
					"type": "screenshare",
					"tooltip": true,
					"tooltip-icon-size": 24
				},
				{
					"type": "audio-out",
					"tooltip": true,
					"tooltip-icon-size": 24
				},
				{
					"type": "audio-in",
					"tooltip": true,
					"tooltip-icon-size": 24
				}
			]
		},
		"network": {
			"format": "{ifname}",
			"format-wifi": "  {essid} ({signalStrength}%)",
			"format-ethernet": "󰈀  {ifname}",
			"format-disconnected": "OFFLINE",
			"tooltip-format": "󰊗  {ifname} via {gwaddr}",
			"tooltip-format-wifi": " {ifname}\n󰩟 {ipaddr}/{cidr}\n󰑩 {gwaddr}\n󰘊 {signaldBm}db\n󱑻 {frequency}MHz",
			"tooltip-format-ethernet": "  {ifname} {ipaddr}/{cidr}",
			"tooltip-format-disconnected": "  Disconnected"
		},
		"pulseaudio": {
			"reverse-mouse-scrolling": true,
			"format": "{icon} {volume:2}%",
			"format-bluetooth": "{icon} 󰂯 {volume:2}%",
			"format-muted": "{icon} 󰖁 MUTE",
			"format-icons": {
				"headphone": "",
				"speaker": "󰓃",
				"hdmi": "󰡁",
				"headset": "󰋎",
				"hands-free": "󰋎",
				"portable": "",
				"car": "",
				"hifi": "󱡬",
				"phone": "",
				"default": [
					"",
					""
				]
			},
			"scroll-step": 5,
			"on-click": "pamixer -t",
			"on-click-right": "pavucontrol"
		},
		"cava": {
			// "cava_config": "$XDG_CONFIG_HOME/cava/config",
			"framerate": 60,
			"autosens": 1,
			"sensitivity": 1.5,
			"bars": 14,
			"lower_cutoff_freq": 80,
			"higher_cutoff_freq": 20000,
			"method": "pipewire",
			"source": "auto",
			"stereo": false,
			"reverse": false,
			"bar_delimiter": 0,
			// "monstercat": false,
			// "waves": false,
			"noise_reduction": 0.77,
			// "input_delay": 2,
			"format-icons": [
				"<span color='#2268bd'>▁</span>", // blue
				"<span color='#2268bd'>▂</span>", // blue
				"<span color='#2268bd'>▃</span>", // blue
				"<span color='#2268bd'>▄</span>", // blue
				"<span color='#2268bd'>▅</span>", // blue
				"<span color='#ff9977'>▆</span>", // orange
				"<span color='#ff9977'>█</span>", // orange
				"<span color='#dd532e'>█</span>" // red
			],
			"actions": {
				"on-click-right": "mode"
			}
		},
		"battery": {
			"states": {
				"good": 80,
				"warning": 30,
				"critical": 15
			},
			"format": "{icon}  {capacity:2}%",
			"format-icons": [
				"",
				"",
				"",
				"",
				""
			]
		},
		"upower": {
			// "icon-size": 24,
			"show-icon": false,
			"hide-if-empty": false,
			"tooltip": false,
			"tooltip-spacing": 20,
			"format": "{time}"
		},
		"tray": {
			"icon-size": 24,
			"spacing": 10,
			"show-passive-items": true
		}
	},
	{
		"layer": "top",
		"position": "bottom",
		"height": 28,
		"modules-left": [
			"disk#root",
			"custom/seperator",
			"disk#tmp",
			"custom/seperator",
			"memory",
			"custom/seperator",
			"cpu",
			"custom/seperator",
			"temperature",
			"custom/right-arrow-dark",
			"custom/right-arrow-light",
			"network",
			"custom/right-arrow-dark"
		],
		"modules-center": [],
		"modules-right": [],

		"custom/seperator": {
			"format": "|",
			"tooltip": false
		},
		"custom/left-arrow-dark": {
			"format": "",
			"tooltip": false
		},
		"custom/left-arrow-light": {
			"format": "",
			"tooltip": false
		},
		"custom/right-arrow-dark": {
			"format": "",
			"tooltip": false
		},
		"custom/right-arrow-light": {
			"format": "",
			"tooltip": false
		},
		"network": {
			"interval": 5,
			// "format": ""
			"format-wifi": "  󰕒{bandwidthUpBits} 󰇚{bandwidthDownBits}",
			"format-ethernet": "󰈀  <span color='#2aa9ff'>󰕒{bandwidthUpBits}</span> <span color='#69ff94'>󰇚{bandwidthDownBits}</span>",
		},
		"memory": {
			"interval": 5,
			"format": "  {icon} {percentage}%",
			"tooltip-format": "  RAM {used:0.1f}G/{total:0.1f}G",
			"format-icons": [
				"<span color='#69ff94'>▁</span>", // green
				"<span color='#2aa9ff'>▂</span>", // blue
				"<span color='#f8f8f2'>▃</span>", // white
				"<span color='#f8f8f2'>▄</span>", // white
				"<span color='#ffffa5'>▅</span>", // yellow
				"<span color='#ffffa5'>▆</span>", // yellow
				"<span color='#ff9977'>▇</span>", // orange
				"<span color='#dd532e'>█</span>" // red
			]
		},
		"cpu": {
			"interval": 1,
			"format": "  {usage:2}% {icon}",
			"format-icons": [
				"<span color='#69ff94'>▁</span>", // green
				"<span color='#69ff94'>▂</span>", // green
				"<span color='#69ff94'>▃</span>", // green
				"<span color='#2aa9ff'>▄</span>", // blue
				"<span color='#2aa9ff'>▅</span>", // blue
				"<span color='#ffffa5'>▆</span>", // yellow
				"<span color='#ff9977'>▇</span>", // orange
				"<span color='#dd532e'>█</span>" // red
			]
		},
		"temperature": {
			// "thermal-zone": 2,
			// "hwmon-path": "/sys/class/hwmon/hwmon2/temp1_input",
			"critical-threshold": 80,
			// "format-critical": "{temperatureC}°C ",
			"format": " {temperatureF}°F {icon}",
			"tooltip-format": " TEMP {temperatureF}°F",
			"format-icons": [
				"<span color='#69ff94'>▁</span>", // green
				"<span color='#69ff94'>▂</span>", // green
				"<span color='#69ff94'>▃</span>", // green
				"<span color='#2aa9ff'>▄</span>", // blue
				"<span color='#2aa9ff'>▅</span>", // blue
				"<span color='#ffffa5'>▆</span>", // yellow
				"<span color='#ff9977'>▇</span>", // orange
				"<span color='#dd532e'>█</span>" // red
			]
		},
		"disk#root": {
			"interval": 5,
			"format": " {path} {percentage_used:2}%",
			"tooltip-format": "  DISK {used}/{total}",
			"path": "/",
		},
		"disk#tmp": {
			"interval": 5,
			"format": " {path} {percentage_used:2}%",
			"tooltip-format": "  DISK {used}/{total}",
			"path": "/tmp",
		},
	},
	{
		// bar 3
		"layer": "top",
		"position": "bottom",
		"height": 28,
		"modules-left": [
			"hyprland/window",
			"custom/right-arrow-dark"
		],
		"modules-center": [],
		"modules-right": [
			"custom/left-arrow-dark",
			"hyprland/language",
			"custom/left-arrow-light",
			"custom/left-arrow-dark",
			"keyboard-state",
		],
		"custom/left-arrow-dark": {
			"format": "",
			"tooltip": false
		},
		"custom/left-arrow-light": {
			"format": "",
			"tooltip": false
		},
		"custom/right-arrow-dark": {
			"format": "",
			"tooltip": false
		},
		"custom/right-arrow-light": {
			"format": "",
			"tooltip": false
		},
		// LEFT
		"hyprland/window": {
			"format": " {}",
			// "max-length": 60,
			"separate-outputs": true,
			"tooltip": false,
			"rewrite": {
				"(.*) - Mozilla Firefox": " $1",
				"(.*) - fish": " [$1]"
			}
		},
		"hyprland/language": {
			"format": "  {}"
		},
		// RIGHT
		"keyboard-state": {
			// "numlock": true,
			"capslock": true,
			// "scrolllock": true,
			"format": {
				"numlock": "󰎦  {icon}",
				// "capslock": "󰪛  {icon}",
				// "scrolllock": "󰜰  {icon}"
			},
			// "format": "{name} {icon}",
			"format-icons": {
				"locked": "",
				"unlocked": ""
			}
		}
	}
]
